<div class="run-changesets">
    <div class="run-changeset">
        <strong class="num">{{this.commitCount}}</strong>
        <span class="changes commitLabel">commit{{#ifCond this.commitCount '>' 1}}s{{/ifCond}}</span>
    </div>
    {{#ifCond this.commitCount '==' 1}}
    <div class="run-changeset">
        <div class="changeset-author"> Author </div>
        <div class="changeset-author-value"> {{contributors.0.authorJenkinsId}} </div>
    </div>
    {{/ifCond}}
</div>